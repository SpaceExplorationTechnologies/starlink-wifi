/dts-v1/;
#include "ipq807x-soc.dtsi"
#include <dt-bindings/qcom/gpio-ipq807x.h>
/ {
	model ="QCA, IPQ807x-HK06";
	compatible = "qca,ipq807x", "qca,ipq807x-hk06";
	machid = <0x08010003>;
	config_name = "config@hk06";

	aliases {
		console = "/serial@78B3000";
		uart2 = "/serial@78B0000";
		i2c0 = "/i2c@78b6000";
		pci0 = "/pci@20000000";
		pci1 = "/pci@10000000";
	};

	ess-switch {
		switch_mac_mode = <0x0>;
		switch_mac_mode1 = <0xFF>;
		switch_mac_mode2 = <0x2>;
		aquantia_port = <5>;
		aquantia_gpio = <63>;
	};
};

