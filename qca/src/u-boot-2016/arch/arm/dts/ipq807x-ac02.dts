/dts-v1/;
#include "ipq807x-soc.dtsi"
#include <dt-bindings/qcom/gpio-ipq807x.h>
/ {
	model ="QCA, IPQ807x-AC02";
	compatible = "qca,ipq807x", "qca,ipq807x-ac02";
	machid = <0x0801000A>;
	config_name = "config@ac02";

	aliases {
		console = "/serial@78B3000";
		uart2 = "/serial@78B0000";
		pci0 = "/pci@20000000";
	};
	ess-switch {
		switch_mac_mode = <0x0>;
		switch_mac_mode1 = <0x6>;
		switch_mac_mode2 = <0x6>;
		napa_gpio = <25 44>;
		napa_gpio_cnt = <2>;
		port_phyinfo {
			port@0 {
				phy_address = <0>;
				phy_type = <1>;
			};
			port@1 {
				phy_address = <1>;
				phy_type = <1>;
			};
			port@2 {
				phy_address = <2>;
				phy_type = <1>;
			};
			port@3 {
				phy_address = <3>;
				phy_type = <1>;
			};
			port@4 {
				phy_address = <24>;
				phy_type = <2>;
			};
			port@5 {
				phy_address = <28>;
				phy_type = <2>;
			};
		};
	};
};

